{
  "manifest_version": 2,

  "name": "Alert",
  "description": "Instagram Alert Message",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html",
	"default_title": "Mavrck IG-Tool",
	"default_icon": "popup.png"
  },
    "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
	"notifications",
    "activeTab",
    "storage"
  ],
   "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["myScript.js", "jquery-3.2.1.min.js"]
    }
  ]
}
