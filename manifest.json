{
  "manifest_version": 2,

  "name": "Mavrck Marktr",
  "description": "Pulls data from instagram profiles",
  "version": "2.0.2",

  "browser_action": {
    "default_popup": "popup.html",
  	"default_title": "Mavrck IG-Tool",
  	"default_icon": "popup.png"
  },

  "permissions": [
	   "notifications",
     "activeTab",
     "storage",
     "identity",
     "tabs"
  ],

  "content_security_policy": "script-src 'self' http://app.splashscore.com; object-src 'self'"
}
